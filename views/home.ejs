<h1>The home page</h1>
<section id="feed-form">
   <h4>Posts</h4>
   <form action="/posts/create" method="post" id="new-feed-form">
    <textarea name="content" cols="40" rows="5" placeholder="Type Here..."></textarea>
    <input type="submit" value="Post">

   </form>
   <div>
    <ul>
        <%for(post of posts){%>
            <li>
                <p><%=post.content%></p>
                <p><%=post.user.name%></p>
            </li>
            
            
        <%}%>
    </ul>
   </div>
</section>